{
    "name": "api",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "nest build",
        "dev": "nest start --watch",
        "start": "nest start",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
        "test": "jest",
        "test:watch": "jest --watch",
        "clean": "rm -rf dist"
    },
    "dependencies": {
        "@nestjs/common": "^10.4.0",
        "@nestjs/config": "^3.3.0",
        "@nestjs/core": "^10.4.0",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/passport": "^10.0.3",
        "@nestjs/platform-express": "^10.4.0",
        "@nestjs/platform-socket.io": "^10.4.0",
        "@nestjs/swagger": "^7.4.0",
        "@nestjs/throttler": "^6.3.0",
        "@nestjs/websockets": "^10.4.0",
        "@prisma/client": "^6.3.0",
        "@election/shared": "*",
        "bcrypt": "^5.1.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "compression": "^1.7.4",
        "helmet": "^8.0.0",
        "ioredis": "^5.4.0",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.4.0",
        "@nestjs/schematics": "^10.2.0",
        "@nestjs/testing": "^10.4.0",
        "@types/bcrypt": "^5.0.2",
        "@types/compression": "^1.7.5",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.0.0",
        "@types/passport-jwt": "^4.0.1",
        "jest": "^29.7.0",
        "prisma": "^6.3.0",
        "source-map-support": "^0.5.21",
        "ts-jest": "^29.2.0",
        "ts-loader": "^9.5.0",
        "ts-node": "^10.9.0",
        "typescript": "^5.7.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "prisma": {
        "seed": "ts-node prisma/seed.ts"
    }
}